      "source": [
        "def remove_stopwords(text):\n",
        "  return [word for word in text if word not in stop_words]\n",
        "\n",
        "df['filtering'] = df['tokenize'].apply(lambda x: remove_stopwords(x))\n",
        "print('filtering : \\n')\n",
        "print(df['filtering'].head())\n",
        "print(\"\\n\\n\\n\")"
      ]
    },
