        "def case_folding(text):\n",
        "  if isinstance(text, str):\n",
        "    lowercase_text = text.lower()\n",
        "    return lowercase_text\n",
        "\n",
        "df['case_folding'] = df['cleaning'].apply(case_folding)\n",
        "print('case_folding : \\n')\n",
        "print(df['case_folding'].head())\n",
        "print(\"\\n\\n\\n\")"
      ]
